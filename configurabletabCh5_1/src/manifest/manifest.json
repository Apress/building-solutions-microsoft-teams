{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.6/MicrosoftTeams.schema.json",
    "manifestVersion": "1.6",
    "id": "{{APPLICATION_ID}}",
    "version": "{{VERSION}}",
    "packageName": "{{PACKAGE_NAME}}",
    "developer": {
        "name": "JPOWER4",
        "websiteUrl": "https://{{HOSTNAME}}",
        "privacyUrl": "https://{{HOSTNAME}}/privacy.html",
        "termsOfUseUrl": "https://{{HOSTNAME}}/tou.html"
    },
    "name": {
        "short": "configurabletabCh5",
        "full": "configurabletabCh5"
    },
    "description": {
        "short": "How to create a custom tab for channel or group chat",
        "full": "This exercise will guide you on how to create a custom tab for channel or group chat"
    },
    "icons": {
        "outline": "icon-outline.png",
        "color": "icon-color.png"
    },
    "accentColor": "#D85028",
    "configurableTabs": [{
        "configurationUrl": "https://{{HOSTNAME}}/myTeamTab/config.html",
        "canUpdateConfiguration": true,
        "scopes": [
            "team",
            "groupchat"
        ]
    }],
    "staticTabs": [],
    "bots": [],
    "connectors": [],
    "composeExtensions": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "{{HOSTNAME}}"
    ],
    "showLoadingIndicator": false
}